    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="number"
            android:ems="10"
            android:id="@+id/editText"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="@+id/editText2"
            android:layout_marginTop="103dp" app:layout_constraintEnd_toEndOf="@+id/editText2"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="number"
            android:ems="10"
            android:id="@+id/editText2"
